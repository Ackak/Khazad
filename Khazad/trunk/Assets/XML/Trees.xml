<Trees xmlns="namespace">
	<!-- ALL trees that can be assigned require a matgloss field. tree without matgloss acts as a default -->
	<Tree>
		<Name Label="TREE_SAGUARO"/>
		<MatGloss value="SAGUARO" />
		<Model file="Assets/Models/Saguaro/Saguaro.obj" />
		<Variant>
		  <Trunk material="MATERIAL_SAGUARO" />
		</Variant>
	</Tree>
	<Tree>
		<Name Label="TREE_OAK"/>
		<MatGloss value="ALDER" />
		<Model file="Assets/Models/oak/oak_leaves_snow.obj" />
		<Variant>
		  <Trunk material="MATERIAL_OAK_TRUNK" />
		  <Snow material="MATERIAL_OAK_SNOW" />
		  <Leaves material="MATERIAL_OAK_LEAVES" />
		</Variant>
	</Tree>
</Trees>
